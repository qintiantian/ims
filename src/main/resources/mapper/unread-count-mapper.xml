<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.konglk.mappers.UnreadCountDao">
    <insert id="insertUnreadCount" parameterType="com.konglk.entity.UnreadCountVO">
      insert into ims_unread_count(unread_count_id,conversation_id, user_id,unread_count,last_msg_id)
      values(#{unreadCountId},#{conversationId}, #{userId},#{unreadCount},#{lastMsgId} )
    </insert>



</mapper>